{
  "name": "Bookee",
  "version": "1.0.0",
  "description": "Ebook Management Application",
  "main": "public/main.js",
  "scripts": {
    "preinstallad": "node preinstall",
    "build": "electron-packager . AXIOS_STUDIO --platform win32 --arch ia32 --ignore node_modules_old --out ./dist/",
    "build_asar": "electron-packager . AXIOS_STUDIO --platform win32 --arch ia32 --ignore node_modules_old --asar --out ./dist/",
    "package": "asar pack . ./studio.asar",
    "installer": "electron-installer-windows --src ./dist/app-win32-x64/ --config ./config.json",
    "webpack": "webpack --progress --profile --colors --define process.env.NODE_ENV='\"development\"' --display-error-details --display-cached --watch",
    "webpack_production": "webpack --progress -p --define process.env.NODE_ENV='\"production\"' --profile --colors --display-error-details --display-cached",
    "start": "set NODE_ENV=production && electron .",
    "start_dev": "set NODE_ENV=development && electron .",
    "jscs": "jscs index.js",
    "watch_dev": "./node_modules/.bin/webpack-dev-server && set NODE_ENV=development",
    "watch": "./node_modules/.bin/webpack-dev-server && set NODE_ENV=production"
  },
  "author": "Akhil Dabral",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css": "^2.2.1",
    "css-loader": "^0.27.3",
    "electron": "^1.6.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "style": "0.0.3",
    "style-loader": "^0.16.0",
    "transform-runtime": "0.0.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "electron-react-devtools": "^0.4.0",
    "pdfviewer": "^1.0.2",
    "radium": "^0.18.2",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0"
  }
}
